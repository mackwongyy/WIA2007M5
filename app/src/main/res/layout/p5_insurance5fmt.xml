        android:layout_height="24dp"
        android:inputType="number"
        android:text="@string/RM0"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="@+id/Insurance1FMTView"
        app:layout_constraintHorizontal_bias="0.834"
        app:layout_constraintStart_toEndOf="@+id/LifeInsurance"
        app:layout_constraintTop_toTopOf="@+id/motorInsuranceDeductiblesLabel"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/personalInsuranceCostLabel"
        android:layout_width="96dp"
        android:layout_height="24dp"
        android:inputType="number"
        android:text="@string/RM0"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="@+id/Insurance1FMTView"
        app:layout_constraintHorizontal_bias="0.834"
        app:layout_constraintStart_toEndOf="@+id/LifeInsurance"
        app:layout_constraintTop_toTopOf="@+id/personalInsuranceDeductiblesLabel"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/medicalInsuranceCostLabel"
        android:layout_width="96dp"
        android:layout_height="24dp"
        android:inputType="number"
        android:text="@string/RM0"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="@+id/Insurance1FMTView"
        app:layout_constraintHorizontal_bias="0.834"
        app:layout_constraintStart_toEndOf="@+id/LifeInsurance"
        app:layout_constraintTop_toTopOf="@+id/medicalInsuranceDeductiblesLabel"
