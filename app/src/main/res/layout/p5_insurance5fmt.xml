        android:layout_height="24dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="48dp"
        android:text="Life"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/Insurance1FMTView"
        app:layout_constraintTop_toBottomOf="@+id/lifeInsuranceDeductible" />

    <TextView
        android:id="@+id/lifeInsuranceDeductiblesLabel"
        android:layout_width="96dp"
        android:layout_height="24dp"
        android:inputType="number"
        android:text="@string/RM0"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="@+id/Insurance1FMTView"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/LifeInsurance"
        app:layout_constraintTop_toTopOf="@+id/LifeInsurance"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/MotorInsurance"
        android:layout_width="72dp"
        android:layout_height="24dp"
        android:layout_marginStart="28dp"
        android:text="Motor"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/Insurance1FMTView"
        app:layout_constraintTop_toBottomOf="@+id/LifeInsurance" />

    <TextView
        android:id="@+id/motorInsuranceDeductiblesLabel"
        android:layout_width="96dp"
        android:layout_height="24dp"
        android:inputType="number"
        android:text="@string/RM0"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="@+id/Insurance1FMTView"
        app:layout_constraintHorizontal_bias="0.0"
