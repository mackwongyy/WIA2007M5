        app:layout_constraintEnd_toEndOf="@+id/Insurance1FMTView"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/LifeInsurance"
        app:layout_constraintTop_toTopOf="@+id/PersonalInsurance"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/medicalInsurance"
        android:layout_width="72dp"
        android:layout_height="24dp"
        android:layout_marginStart="28dp"
        android:text="Medical"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/Insurance1FMTView"
        app:layout_constraintTop_toBottomOf="@+id/PersonalInsurance" />

    <TextView
        android:id="@+id/medicalInsuranceDeductiblesLabel"
        android:layout_width="96dp"
        android:layout_height="24dp"
        android:inputType="number"
        android:text="@string/RM0"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="@+id/Insurance1FMTView"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/LifeInsurance"
        app:layout_constraintTop_toTopOf="@+id/medicalInsurance"
